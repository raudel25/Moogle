@page "/{id}/{pos:int}"

<PageTitle>Moogle!</PageTitle>

<h1>🔍 Moogle!</h1>
<h3>@id</h3>

@foreach (var s in book)
{
    <p>@s</p>
}
@code {
    [Parameter]
    public string id { get; set; }
    [Parameter]
    public int pos {get; set;} 
    private List<string> book  {get{return Server.leer(id,pos);}} 
    private void Next()
    {
        pos=0;
    }
}
